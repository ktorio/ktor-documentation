{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "schedule:monthly"
  ],
  "packageRules": [
    {
      "description": "Disable changelogs fetching for Ktor dependencies",
      "matchPackageNames": [
        "io.ktor:*"
      ],
      "fetchChangeLogs": "off"
    }
  ]
}
